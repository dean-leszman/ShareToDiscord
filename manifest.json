{
	"name": "Share to Discord",
	"version": "0.1.0",
	"manifest_version": 2,
	"description": "Share content directly to Discord",
	"permissions": [
		"contextMenus",
		"https://www.discordapp.com/*",
		"https://discord.gg/*"
	],
	"background": {
		"scripts": ["script.js"]
	},
	"icons": {
		"16": "logo_16.jpg",
		"48": "logo_48.jpg",
		"128": "logo_128.jpg"
	}
}